    </item>
    <item android:state_enabled="false">
        <shape>
            <corners android:radius="3dp" />
            <solid android:color="#66d5d5d5" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="3dp" />
            <solid android:color="#d5d5d5" />
        </shape>
    </item>
</selector>